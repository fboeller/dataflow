{
  "name": "Get GitHub issues of a repo",
  "steps": [
    {
      "assignTo": "issueResponse",
      "request": {
        "method": "GET",
        "url": "http://api.github.com/repos/fboeller/dataflow/issues",
        "body": null,
        "headers": null
      }
    },
    {
      "assignTo": "titles",
      "expression": {
        "$map": {
          "$eval": "issueResponse.body"
        },
        "each(x)": {
          "$eval": "x.title"
        }
      }
    },
    {
      "assignTo": "issues",
      "expression": {
        "$eval": "issueResponse.body"
      }
    },
    {
      "assignTo": "titles2",
      "for": {
        "const": "issue",
        "in": "issues",
        "do": [
          {
            "assignTo": "title",
            "expression": {
              "$eval": "issue.title"
            }
          }
        ],
        "return": "title"
      }
    }
  ]
}